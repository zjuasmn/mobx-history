{
  "name": "mobx-history",
  "version": "1.0.1",
  "description": "Mobx History",
  "main": "index.js",
  "repository": "zjuasmn/mobx-history",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha src/__test__",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build-lib": "babel src -d . --ignore __test__",
    "build-umd": "webpack src/index.js umd/mobx-history.js",
    "build-min": "webpack -p src/index.js umd/mobx-history.min.js",
    "build": "node ./scripts/build.js",
    "prerelease": "npm run build",
    "release": "node ./scripts/release.js"
  },
  "files": [
    "*.js",
    "!*.config.js",
    "umd"
  ],
  "keywords": [
    "mobx",
    "history"
  ],
  "author": "zjuasmn <zjuasmn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.6.1",
    "history": "^4.5.1",
    "mobx": "^3.1.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.4",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "webpack": "^2.2.1",
    "readline-sync": "^1.4.6"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
