{
  "name": "mobx-history",
  "version": "2.0.0",
  "description": "Mobx History",
  "main": "index.js",
  "repository": "zjuasmn/mobx-history",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha src/__test__",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build-lib": "babel src -d . --ignore __test__",
    "build-umd": "webpack src/index.js umd/mobx-history.js",
    "build-min": "webpack -p src/index.js umd/mobx-history.min.js",
    "build": "node ./scripts/build.js",
    "prerelease": "npm run build",
    "release": "node ./scripts/release.js"
  },
  "files": [
    "*.js",
    "!*.config.js",
    "umd"
  ],
  "keywords": [
    "mobx",
    "history"
  ],
  "author": "zjuasmn <zjuasmn@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "history": "^4.7.2"
  },
  "peerDependencies": {
    "mobx": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "istanbul": "^0.4.5",
    "mobx": "^4.1.1",
    "mocha": "^5.1.0",
    "nyc": "^11.6.0",
    "readline-sync": "^1.4.9",
    "webpack": "^2.2.1"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
